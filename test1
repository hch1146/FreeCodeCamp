//Where do I belong
function where(arr, num) {
  // Find my place in this sorted array.
  arr=arr.sort(function(a, b) {
    return a - b;
  });
    for(var i=0;i<arr.length;i++){
      if(num===arr[i]){
       return i; 
      }
     else if(num>arr[i]&&num<arr[i+1]){
      return i+1;
    }
      else if(num>arr[arr.length-1]){
        return arr.length;
      }
   } 
}

where([40, 60], 50);
